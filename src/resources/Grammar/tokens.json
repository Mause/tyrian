{
    "literal": {
        "(": "OPEN_BRACKET",
        ")": "CLOSE_BRACKET",
        ";": "SEMICOLON",

        "\"": "DOUBLE_QUOTE",
        "`": "SHARP_QUOTE",
        "'": "SINGLE_QUOTE",

        "\n": "NEWLINE",
        ".": "DOT",
        "#": "HASH",
        ",": "COMMA"
    },
    "regex": {
        "([+-]?\\d+)": "NUMBER_RE",
        "([_a-zA-Z\\-]+)": "ID_RE",
        "([\\w,./]+)": "STRING_RE",
        "([+/*-])": "SYMBOL_RE"
    }
}
