{
    "literal": {
        "(": "OPEN_BRACKET",
        ")": "CLOSE_BRACKET",
        ";": "SEMICOLON",

        "\"": "DOUBLE_QUOTE",
        "`": "SHARP_QUOTE",
        "'": "SINGLE_QUOTE",

        "\n": "NEWLINE",
        ".": "DOT",
        "#": "HASH"
    },
    "regex": {
        "[+-]?\\d+[-+]?": "NUMBER",
        "(([*+])[a-zA-Z\\-]+\\2)|[a-zA-Z\\-]+": "ID",
        "\\w+": "STRING_RE",
        "[,+/\\-*]": "SYMBOL"
    }
}
