%start_token=list;

s_exp: atom
     | list
     ;

atom: NUMBER
    | SYMBOL
    | STRING
    ;

list: OPEN_BRACKET CLOSE_BRACKET   /* empty list () */
    | OPEN_BRACKET s_exp_list CLOSE_BRACKET
    | OPEN_BRACKET s_exp_list DOT s_exp CLOSE_BRACKET
    | SINGLE_QUOTE s_exp
    | SHARP_QUOTE s_exp
    | HASH s_exp
    ;

s_exp_list: s_exp
          | s_exp_list s_exp
          ;
